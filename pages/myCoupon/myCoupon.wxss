.container{
  background-color: #f8f8f8;
  width: 100%;
  padding: 3.62rpx 0 0;
}
.couponTop{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background-color: #fff;
  height: 90.5rpx;
  line-height: 90.5rpx;
  text-align: center;
  border: 1.81rpx solid rgba(255, 255, 255, 0);
  border-bottom: 1.81rpx solid rgba(187, 187, 187, 1);
  /* margin-bottom: 30.77rpx; */
  position: fixed;
  z-index: 5;
}
.mtx{
  margin-top: 121.27rpx;
}
.top-item{
  width: 33.33%;
  height: 90.5rpx;
  line-height: 90.5rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 32.58rpx;
  text-align: center;
  font-family: PingFangSC-regular;
  box-sizing: border-box;
}
.top-text{
  width: 97.74rpx;
  height: 47.06rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 32.58rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.top-text.active{
  color: rgba(230, 88, 93, 1);
}
.top-item.active{
  color: rgba(230, 88, 93, 1);
  border-bottom: 5.43rpx solid rgba(230, 88, 93, 1);
}
.coupon-box{
