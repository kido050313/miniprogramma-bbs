  border: 1.81px solid rgba(255, 255, 255, 0);
}
.img{
  width:141.18rpx;
  height:141.18rpx;
  position:absolute;
  top:108.6rpx;
  left:40.68rpx;
}
.grey.coupon-box{
  border: 1.81rpx solid rgba(185, 185, 185, 1);
}
.grey .box-l{
  border-right: 3.62rpx dashed rgba(185, 185, 185, 1);
}
.grey.coupon-tips{
